1. int fun(int inputarray[],int index)
{
int i;
int n = inputarray.length();
int l = 0;
int r = n-1;
while( l<r)
{
int mid = l+r/2;
if ( inputarray[mid]==index)
{
return mid'
}
else if( inputarray[i]<index
}
l = mid+1;
else
{
l = mid-1;
}
}
return -1;


