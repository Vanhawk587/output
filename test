4.
string max( string st)
{
string mt =  st;
int n = st.length();
sort( mt.begin(),mt.end())
char a = mt[0];
char b   = mt[n-1];
for(int i = 0;i<st.length();i++)
{
if ( st[i] == a)
{
k = i;
}
if ( st[i] == b)
{
l = i;
}
swap( st[k],st[l]);
return st;
   


}



